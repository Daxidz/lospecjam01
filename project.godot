; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "RigidBody2D",
"class": "Noeud",
"language": "GDScript",
"path": "res://src/fighter/Noeud.gd"
} ]
_global_script_class_icons={
"Noeud": ""
}

[application]

config/name="LospecJam01"
run/main_scene="res://src/menu/MenuPrincipal.tscn"
config/icon="res://icon.png"

[autoload]

InputConfig="*res://src/menu/InputConfig.gd"
GameOptions="*res://Globals/GameOptions.gd"
ControllerManager="*res://Globals/ControllerManager.gd"
Sounds="*res://Globals/Sounds.tscn"
SceneSwitcher="*res://Globals/SceneSwitcherr.tscn"

[display]

window/size/width=240
window/size/height=135
window/size/test_width=1920
window/size/test_height=1080
window/stretch/mode="viewport"
window/stretch/aspect="keep"

